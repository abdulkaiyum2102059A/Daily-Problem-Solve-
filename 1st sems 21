#include <stdio.h>

// Define the Complex structure
struct Complex {
    float x;
    float y;
};

int main() {
    // Declare a variable P of type Complex with initial values 0.0 and 1.1
    struct Complex P = {0.0, 1.1};

    // Access and print the values of the members of the Complex variable P
    printf("x: %.2f\n", P.x); // Output: x: 0.00
    printf("y: %.2f\n", P.y); // Output: y: 1.10

    return 0;
}

